{
  "name": "@nomadprogrammer/likha-plugins",
  "version": "0.0.1",
  "description": "Official plugins for Likha editor",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build && tsc --emitDeclarationOnly",
    "dev": "vite build --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "editor",
    "plugins",
    "rich-text"
  ],
  "author": "Shiv Singh <shiv@srapsware.com> (https://github.com/ProgrammerNomad)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ProgrammerNomad/likha.git",
    "directory": "packages/plugins"
  },
  "homepage": "https://github.com/ProgrammerNomad/likha#readme",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@nomadprogrammer/likha-core": "^0.0.1",
    "prosemirror-commands": "^1.5.2",
    "prosemirror-inputrules": "^1.5.1",
    "prosemirror-keymap": "^1.2.2",
    "prosemirror-model": "^1.19.4",
    "prosemirror-schema-list": "^1.5.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vitest": "^1.0.0"
  }
}
